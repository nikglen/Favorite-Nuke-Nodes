 }
set N14163090 [stack 0]
 FilterErode {
  size {{parent.inpaintErode} {parent.erodeY}}
  name FilterErode1
  xpos -40
  ypos 452
 }
 Crop {
  box {0 0 4604 2428}
  name Crop1
