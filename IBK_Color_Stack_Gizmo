 Dot {
  name Dot1
  tile_color 0x9597bf00
  xpos -439
  ypos -24
 }
 Grade {
  multiply {{mult.r} {mult.g} {mult.b} {curve}}
  add {{off.r} {off.g} {off.b} {curve}}
  name Grade11
  tile_color 0x7aa9ff00
  xpos -473
  ypos 27
 }
 Clamp {
  maximum_enable false
  name Clamp2
  xpos -473
  ypos 88
 }
set N140f1b70 [stack 0]
push $N140f1b70
push $N140f1b70
